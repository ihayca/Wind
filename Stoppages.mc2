           $initrow [ $init 3600000000 / ] APPEND 'initrow' STORE
           $stoprow [ $stop 3600000000 / ] APPEND 'stoprow' STORE
           $errorrow [ $error 3600000000 / ] APPEND 'errorrow' STORE
           $servicerow [ $service 3600000000 / ] APPEND 'servicerow' STORE
           $lowrow [ $low 3600000000 / ] APPEND 'lowrow' STORE
