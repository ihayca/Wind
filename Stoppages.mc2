        %> FOR
     $newrows [ $startrow ] APPEND [ $offlinerow ] APPEND [ $initrow ] APPEND [ $stoprow ] APPEND [ $errorrow ] APPEND [ $servicerow ] APPEND [ $lowrow ] APPEND [ $highrow ] APPEND  'newrows' STORE

       {
          'columns' $newcolumns //[ 'Status' ]
          'rows' $newrows
        } 
        'values' STORE
    
        { 'data' $values
