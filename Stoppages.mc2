        'eventHandler' 'type=variable,tag=(status_event|status_code|plant|TurbineId)'
      }
      'x' 11 'y' 0  'w' 1 'h' 1
      'macro' <% 
        { 
          'data' <%  
            $status_event '|' SPLIT 'outageList' STORE
