                            1500.0 1500.0 1500.0 1500.0 1500.0 1500.0 1500.0
                            1500.0 1500.0 ] // values
              MAKEGTS $max_wind MINLONG TIMECLIP 'theoretical' STORE
  
[ $bucketized bucketizer.sum 0 0 1 ] BUCKETIZE VALUES 0 GET 0 GET 
