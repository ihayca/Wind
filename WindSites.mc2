      'eventHandler' 'type=variable,tag=(plant|startDate|endDate)'  // suscribe to events
                        }
      'macro' <%
 [ $myreadtoken '~.*' { 'plant' $plant } ] FINDSETS STACKTOLIST 1 GET 'TurbineId' GET 'listOfTurbines' STORE 
$listOfTurbines SIZE 'numTurbines' STORE
[] 'turbinelist' STORE
[] 'pwrlist' STORE
[] 'windlist' STORE
[] 'timelist' STORE

//FOR plant turbines
1 $numTurbines <% 
  'turbineid' STORE 
  //get total production
     [ $myreadtoken '~(ACTIVE_POWER|WIND_SPEED).*' { 'plant' $plant 'TurbineId' $turbineid TOSTRING } 
         $startDate TOSTRING $endDate TOSTRING
      ] FETCH 'inputs' STORE //get all wind speed for a period

    <% $inputs [] == %>
    <%  '0' 'curvescore'  STORE
        '0' 'act_pwr'  STORE
        '0' 'act_wind'   STORE
        '0' 'time_avail'  STORE  %>
    <%  
     $inputs LASTTICK  "lastBucket" STORE
     10 m "bucketSpan" STORE // 1 minute
     $inputs FIRSTTICK "start" STORE // count = ((end - start) / bucketspan) + 1
     $lastBucket $start - $bucketSpan / TOLONG 1 + "bucketCount" STORE
     [ $inputs bucketizer.mean $lastBucket $bucketSpan $bucketCount ] BUCKETIZE 'bucket' STORE
     $bucket [ NaN NaN NaN 0 ] FILLVALUE 'bucket' STORE // fill all empty ticks with zero

     $bucket 0 GET 'wind' STORE
     $bucket 1 GET 'pow' STORE


// reduce all turbine totals to totalsum, zero if no energy
<% $pow [] == %>
<% 0.0 'act_pwr' STORE %>
<% [ $pow bucketizer.last NOW MAXLONG d 0 ] BUCKETIZE 'act_pwr' STORE // sum of each turbine seperately and aligned
$act_pwr VALUES 0 GET 0 GET 'act_pwr' STORE %>
 1 SWITCH 
<'
%.2f
'> 
[ $act_pwr ] STRINGFORMAT 'act_pwr' STORE

// calculate active wind 

<% $wind [] == %>
<% 0.0 'act_wind' STORE %>
<% [ $wind bucketizer.last NOW MAXLONG d 0 ] BUCKETIZE 'act_wind' STORE 
$act_wind VALUES 0 GET 0 GET 'act_wind' STORE %>
 1 SWITCH 
<'
%.2f
'> 
[ $act_wind ] STRINGFORMAT 'act_wind' STORE

// time availability of the turbine
      // check if no data
<% $wind [] == %>
<% 0.0 'time_avail' STORE %>
<%  
  [ $wind bucketizer.mean 0 1 m 0 ] BUCKETIZE 'wind_data' STORE //align timestamps
    [ $pow bucketizer.mean 0 1 m 0 ] BUCKETIZE 'power_data' STORE //align timestamps
     
    [ $wind_data bucketizer.count 0 0 1 ] BUCKETIZE 'all_data_count' STORE  //all points count
    [ $wind_data [] 2.5 mapper.ge 0 0 0 ] MAP 0 GET 0 * 'wind_ge_data' STORE // points where wind speed is greater than cut-in
    [ $wind_data [] 30 mapper.le 0 0 0 ] MAP 0 GET 0 *  'wind_le_data' STORE // points where wind speed is less than cut-in
    $wind_data 0 GET $wind_ge_data + $wind_le_data + 0 * 'wind_window' STORE //get the filtered series, times when there is available wind
    
    
    
     [ $power_data [] 0 mapper.gt 0 0 0 ] MAP 0 GET 0 * 'power_available' STORE  // points where power is greater than 0
    $power_data 0 GET $power_available + 'power_window' STORE
    
      $power_window $wind_window + 'available_window' STORE  // times when there is wind and power together

    //[ $available_window bucketizer.count 0 0 1 ] BUCKETIZE 'avail_data_count' STORE  //wind available points count
    
    //100 $avail_data_count VALUES 0 GET 0 GET TODOUBLE $all_data_count VALUES 0 GET 0 GET TODOUBLE  / * 'time_avail' STORE  
    100 $available_window SIZE TODOUBLE $wind_window SIZE TODOUBLE  / * 'time_avail' STORE     
  %>
 1 SWITCH 

// calculate turbine perf according to vendor power curve
<% $plant 'Soke' == %>
<% 
$wind 100 * 'wind' STORE
[ $pow bucketizer.mean 0 1 m 0 ] BUCKETIZE 'pow' STORE
[ $wind bucketizer.mean 0 1 m 0 ] BUCKETIZE 'wind' STORE

$wind VALUES FLATTEN  // ticks
[] [] []             // latitudes, longitudes and elevations
$pow VALUES FLATTEN // values
MAKEGTS
'data' RENAME 'data' STORE

[ $data bucketizer.mean 0 50 0 ] BUCKETIZE 'bucketized' STORE 
$bucketized LASTTICK 'max_wind' STORE

[ 0 300 350 400 450 500 550 600 650 700 750 800 850 900 950 1000 1050 1100 1150 1200 1250 1300 1350 1400 1450 
1500 1550 1600 1650 1700 1750 1800 1850 1900 1950 2000 2050 2100 2150 2200 2250 2300 2350 2400 2450 2500 ]  // ticks
