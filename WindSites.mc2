%> 1 SWITCH
 %> 1 SWITCH
$turbinelist [ $curvescore ] APPEND 'turbinelist' STORE
$pwrlist [ $act_pwr ] APPEND 'pwrlist' STORE
$windlist [ $act_wind ] APPEND 'windlist' STORE
