{
    'title' 'Active vs Expected Power'
    'options' { 'autoRefresh' 10
                'eventHandler' 'type=variable,tag=(plant|startDate|endDate)'
								'showRangeSelector' true

				}
    'x' 0 'y' 2 'w' 4 'h' 2
    'type' 'line' 'macro' <%
     [ $myreadtoken '~ACTIVE_POWER.*' { 'plant' $plant } 
         $startDate TOSTRING 
		 $endDate TOSTRING
      ] FETCH 'data' STORE
	   $data { '.app' '' '.uuid' '' } RELABEL 'data' STORE //relabel the atributes
     [ $data bucketizer.last 0 10 m 0 ] BUCKETIZE [ SWAP [] reducer.sum ] REDUCE 'data' STORE
	   
     [ $myreadtoken '~WIND_SPEED.*' { 'plant' $plant } 
         $startDate TOSTRING 
		 $endDate TOSTRING
      ] FETCH 'wind_speed_data' STORE
	   $wind_speed_data { '.app' '' '.uuid' '' } RELABEL 'wind_speed_data' STORE //relabel the atributes
	   
    $plant
    <% $plant "Soke" == %> 
    <% $wind_speed_data 0 GET 2.5 < 'winds' STORE
    $winds 0 * 'cut_in_speeds' STORE 

    $wind_speed_data 0 GET 25 > 'winds' STORE
    $winds 0 * 'cut_out_speeds' STORE 

    $wind_speed_data 0 GET 11 >= 25 < 'winds' STORE
    $winds 3450 * $winds / 'max_speeds' STORE 

    $wind_speed_data 0 GET 2.5 >= 11 <= 'winds' STORE
    $winds 3 ** 0.38 * 1.22 * PI * 89.5 * 89.5 * 8 / 1000 / 1.3 * 'rated_speeds' STORE 
    
    $rated_speeds 3450 <= 'rated_norm' STORE

    $rated_speeds 3450 > 'rated_high' STORE
    $rated_high 3450 * $rated_high / 'rated_max' STORE 

    [ $rated_norm $rated_max $cut_in_speeds $cut_out_speeds $max_speeds ] MERGE SORT %>
    
    <% $wind_speed_data 0 GET 2.5 < 'winds' STORE
    $winds 0 * 'cut_in_speeds' STORE 

    $wind_speed_data 0 GET 25 > 'winds' STORE
    $winds 0 * 'cut_out_speeds' STORE 

    $wind_speed_data 0 GET 11 >= 25 < 'winds' STORE
    $winds 1520 * $winds / 'max_speeds' STORE 

    $wind_speed_data 0 GET 2.5 >= 11 <= 'winds' STORE
    $winds 3 ** 0.38 * 1.22 * PI * 89.5 * 89.5 * 8 / 1000 / 1.3 * 'rated_speeds' STORE 
    
    $rated_speeds 1520 <= 'rated_norm' STORE

    $rated_speeds 1520 > 'rated_high' STORE
    $rated_high 1520 * $rated_high / 'rated_max' STORE 

    [ $rated_norm $rated_max $cut_in_speeds $cut_out_speeds $max_speeds ] MERGE SORT %>
    1 SWITCH 'ExpectedAve' RENAME 'expected' STORE
    
      $expected
      $data
      %>
