    <% $plant 'Yalova' != %> <%
     <' 
      %.2f
    '> 
    [ $plant_pwr ] STRINGFORMAT 'plant_pwr' STORE
    %> <%
      $plant_data VALUES 0 GET 0 GET TODOUBLE 1000 / 'plant_pwr' STORE
      <' 
      %.2f
    '> 
    [ $plant_pwr ] STRINGFORMAT 'plant_pwr' STORE
    %>  1 SWITCH
     
       $plant_pwr

    %>
    }

