      <% $plant 'Usak' == %> <%
         [ $myreadtoken '~(GW_PLANT_AVAILABLE_ACTIVE_POWER|SN_TOTAL_POWER_MW).*' { 'plant' $plant } 
             $startDate TOSTRING 
             $endDate TOSTRING
       //NOW 1 d
          ] FETCH 'y' STORE
       %>
