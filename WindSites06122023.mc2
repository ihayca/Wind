    {  
    'title' 'Time Availability'   
    'options' { 'autoRefresh' 600
                'eventHandler' 'type=variable,tag=(plant|startDate|endDate|result)'
				'maxValue' 100
		'unit' 'pct' 
				}
    'x' 4 'y' 1 'w' 2 'h' 1
