    'macro' <%    
      $rows SIZE 'total' STORE
      0.0 'avail' STORE
     0 $rows SIZE 1 - <% 'i' STORE 
                    $rows $i GET 3 GET TODOUBLE $avail + 'avail' STORE
                      %> FOR
       $avail $total / 'time_avail' STORE

          <'
      %.2f
      '> 
      [ $time_avail ] STRINGFORMAT


    %>
